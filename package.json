{
  "name": "crm-foursys-frontend",
  "version": "1.0.0",
  "description": "CRM B2B Tech Foursys - Frontend React Mock",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "dev:host": "vite --host",
    "dev:port": "vite --port 3001",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preview:port": "vite preview --port 3000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directs --max-warnings 0",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "clean:cache": "rimraf node_modules/.vite",
    "start": "npm run dev",
    "start:fresh": "npm run clean:cache && npm run dev"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/x-data-grid": "^6.19.4",
    "@mui/x-date-pickers": "^6.19.4",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.0"
  }
}
